    <xs:simpleType name="AK-CC-NumberType">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                <translation>AK-Nummer Typ</translation>
                <description>
                    <short>Nummer der Ausgleichskasse Typ</short>
                    <technical>
                        Muster: 3-Stellen Punkt 3-Stellen
                        Werte mit drei führenden Nullen sind nicht erlaubt
                        z.B. 000.003 ist nicht korrekt, 003.000 dagegen ist korrekt
                    </technical>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                <translation>Type de numéro AC</translation>
                <description>
                    <short>Type de numéro de la caisse de compensation</short>
                    <technical>
                        Modèle : 3 chiffres Point 3 chiffres
                        Les valeurs avec trois zéros en tête ne sont pas autorisées
                        par exemple, 000.003 n'est pas correct, 003.000 est correct
                    </technical>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="it">
                <translation>Tipo di numero CC</translation>
                <description>
                    <short>Numero della cassa di compensazione</short>
                    <technical>
                        Modello: 3 cifre Punto 3 cifre
                        Non sono ammessi valori con tre zeri iniziali
                        ad es. 000.003 non è corretto, 003.000 è corretto
                    </technical>
                </description>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:pattern value="([1-9][0-9]{2}|[0-9][1-9][0-9]|[0-9]{2}[1-9])\.[0-9]{3}"/>
        </xs:restriction>
    </xs:simpleType>
