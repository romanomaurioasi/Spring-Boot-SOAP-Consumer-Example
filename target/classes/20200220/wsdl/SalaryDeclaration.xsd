    <!-- ** minor version **

        supported minor version (only to validate)
        attribute schemaVersion
    -->
    <xs:simpleType name="SupportedMinorSchemaVersionAttributeType">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                <translation>Unterstützte Minor Schema Versionen</translation>
                <description>
                    <short>Hier werden die kompatiblen Minor Versionen aufgelistet</short>
                    <technical>pattern mehrere Vorkommastellen ermöglichen und führenden Nullen ausschliessen
                        [0-9]\.[0-9] = Major eine Stelle, Punkt, Minor eine Stelle
                        oder
                        [1-9]\d*\.[0-9] = beliebige Major ohne führende Nullen, Punkt, eine Stelle Minor
                    </technical>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                <translation>Versions "Minor" du schéma supportées</translation>
                <description>
                    <short>Liste des versions "Minor" compatibles</short>
                    <technical>Le modèle permet plusieurs places avant le point décimal et exclut les zéros de tête
                        [0-9]\.[0-9] = majeur une place, point, mineur une place
                        ou
                        [1-9]\d*\.[0-9] = tout majeur sans zéros de tête, point, mineur une place
                    </technical>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="it">
                <translation>Versioni di schema minori supportate</translation>
                <description>
                    <short>Le versioni minori compatibili sono elencate qui</short>
                    <technical>pattern consentono più posti prima del punto decimale ed escludono gli zeri iniziali
                        [0-9]\ \ \ 9] = maggiore un posto, punto, minore un posto
                        o
                        [1-9]\d**.[0-9] = qualsiasi maggiore senza zeri iniziali, punto, un posto minore
                    </technical>
                </description>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:pattern value="[0-9]\.[0-9]|[1-9]\d*\.[0-9]"/>
            <xs:enumeration value="0.0"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
