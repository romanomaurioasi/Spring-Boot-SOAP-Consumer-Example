                <translation>Vordefinierte Kategorien</translation>
                <description>
                    <short>Vordefinierte Kategorien</short>
                    <domain>
                        Mögliche Werte:
                        HEN, HEY: VR-Honorar an qsP mit Wohnsitz im Ausland;
                        MEN, MEY: Leistungen aus exportierten Mitarbeiterbeteiligungen an qsP mit Wohnsitz im Ausland;
                        NON, NOY: Nicht quellensteuerpflichtig;
                        SFN: Sondervereinbarung mit Frankreich;
                    </domain>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                <translation>Catégories prédéfinies</translation>
                <description>
                    <short>Catégories prédéfinies</short>
                    <domain>
                        Valeurs possibles:
                        HEN, HEY: Honoraires de membres du CA aux PIS domiciliés à l’étranger;
                        MEN, MEY: Prestations issues de participations de collaborateur en faveur de PIS domiciliées à
                        l’étranger;
                        NON, NOY: Non soumis à l'impôt à la source;
                        SFN: Accord spécial avec la France;
                    </domain>
                </description>
            </xs:documentation>
            <xs:documentation xml:lang="it">
                <translation>Categorie predefinite</translation>
                <description>
                    <short>Categorie predefinite</short>
                    <domain>
                        Valeurs possibili:
                        HEN, HEY: Onorario CdA a PIF con domicilio all'estero;
                        MEN, MEY: Prestazioni derivanti da partecipazioni dei collaboratori esportate a PIF con
                        domicilio all’estero;
                        NON, NOY: Non soggetto all'imposta alla fonte;
                        SFN: Accordo speciale con la Francia;
                    </domain>
                </description>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="HEN"/>
            <xs:enumeration value="HEY"/>
            <xs:enumeration value="MEN"/>
            <xs:enumeration value="MEY"/>
            <xs:enumeration value="NON"/>
            <xs:enumeration value="NOY"/>
            <xs:enumeration value="SFN"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TaxAtSourceCodeType">
        <xs:annotation>
            <xs:documentation xml:lang="de">
                <translation>QST-Code</translation>
            </xs:documentation>
            <xs:documentation xml:lang="fr">
                <translation>Barème IS</translation>
            </xs:documentation>
            <xs:documentation xml:lang="it">
                <translation>Codice tariffario IF</translation>
            </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:NMTOKEN">
            <xs:pattern value="[A-Z][0-9][Y,N]"/>
        </xs:restriction>
    </xs:simpleType>


