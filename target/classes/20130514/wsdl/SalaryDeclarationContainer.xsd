            <xs:extension base="sdc:ResultStateBaseType">
                <xs:sequence>
                    <xs:element name="StatisticQuittanceWithoutCompletion" type="sdc:StatisticQuittanceType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Quittung</translation>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Quittance</translation>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
