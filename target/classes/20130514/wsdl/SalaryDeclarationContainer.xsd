                    <xs:element name="ExpectedAvailability" type="xs:date" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Erwartete Verfügbarkeit</translation>
                                <description>
                                    <short>
                                        Erwartete Verfügbarkeit der Rückmeldung
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Disposition prevue</translation>
                                <description>
                                    <short>Disposition prevue des retours</short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
