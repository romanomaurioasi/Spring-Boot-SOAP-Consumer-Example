    <xs:complexType name="OrganizationRegistrationBaseType">
        <xs:sequence>
            <xs:element name="Institution" type="oa:InstitutionsType"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OrganizationRegistrationType">
        <xs:complexContent>
            <xs:extension base="oa:OrganizationRegistrationBaseType">
                <xs:sequence>
                    <xs:element name="CompanyUID-BFS" type="oa:UID-BFSType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>UnternehmensID BFS</translation>
                                <description>
                                    <short>
                                        Unternehmensidentifikation BFS
                                    </short>
                                    <domain>
                                        neue Unternehmensidentifikation vom BFS;
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Identification de l'entreprise OFS</translation>
                                <description>
                                    <short>
                                        Identification de l'entreprise OFS
                                    </short>
                                    <domain>
                                        Nouvelle identification de l'entreprise OFS
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation>
                                <usage>all</usage>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CompanyName" type="c:CompanyNameType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Name</translation>
                                <description>
                                    <short>
                                        Name des Unternehmens
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Nom</translation>
                                <description>
                                    <short>
                                        Nom de l'entreprise
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation>
                                <usage>all</usage>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="CompanyContactPerson" type="oa:ContactPersonType"/>
                    <xs:element name="AuthorizedFor" type="oa:AuthorizedForCompanyType" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="RegistrationQuittanceType">
        <xs:sequence>
            <xs:element name="X509Subject">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="CommonName" type="xs:string"/>
                        <xs:element name="OrganizationName" type="xs:string"/>
                        <xs:element name="LocalityName" type="xs:string"/>
                        <xs:element name="StateOrProvinceName" type="xs:string"/>
                        <xs:element name="CountryName" type="xs:string"/>
                        <xs:element name="BusinessCategory" type="xs:string" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="CompanyUID-BFS" type="oa:UID-BFSType"/>
            <xs:element name="AuthorizedForUID-BFS" type="oa:UID-BFSType" minOccurs="0"/>
            <xs:element name="Comment" type="c:NotificationsType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="CertificateSignRequestBaseType">
        <xs:sequence>
            <xs:element name="PEM" type="xs:base64Binary">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>signierte Zertifikatsanforderung</translation>
                        <description>
                            <short>signierte Zertifikatsanforderung</short>
                            <technical>Die Struktur des verwendeten elektronischen Zertifikatsantrags ist als PKCS#10
                                standardisiert. Die CSR ist im PEM Format.
                                Network Working Group, 2000. RFC2986: PKCS #10: Certification Request Syntax
                                Specification - Version 1.7, online: https://tools.ietf.org/html/rfc2986 (03.11.2015).
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CertificateSignRequestType">
        <xs:complexContent>
            <xs:extension base="oa:CertificateSignRequestBaseType">
                <xs:sequence>
                    <xs:element name="OneTimePassword" type="oa:NotEmptyStringType"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CertificateSignResponseType">
        <xs:sequence>
            <xs:element name="SubjectDN" type="oa:NotEmptyStringType"/>
            <xs:element name="IssuerDN" type="oa:NotEmptyStringType"/>
            <xs:element name="NotBefore" type="xs:dateTime"/>
            <xs:element name="NotAfter" type="xs:dateTime"/>
            <xs:element name="PEM" type="xs:base64Binary">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Zertifikat</translation>
                        <description>
                            <short>UID-BFS Zertifikat</short>
                            <technical>UID-BFS Zertifikat im PEM Format</technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

