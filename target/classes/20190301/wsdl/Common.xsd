    <xs:complexType name="CompanyNameType">
        <xs:sequence>
            <xs:element name="HR-RC-Name" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Name oder HR-Name</translation>
                        <description>
                            <short>Name oder HR-Name des Unternehmens</short>
                            <domain>Falls eine eingetragener Handelsregisternamen vorhanden ist,
