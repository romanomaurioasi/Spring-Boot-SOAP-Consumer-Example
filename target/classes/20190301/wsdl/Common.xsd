                                <description>
                                    <short>Wohnkanton</short>
                                    <domain>Wohnkanton am Stichtag 31.12 oder beim Austritt. Dieser ist
                                        massgebend für den Lohnausweis.
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Canton de résidence</translation>
                                <description>
                                    <short>Canton de résidence</short>
                                    <domain>Canton de résidence au moment du jour fixé (31.12.) ou au
                                        moment de la sortie. Celui-ci est déterminant pour le certificat de
                                        salaire.
                                    </domain>
                                </description>
                            </xs:documentation>
                            <xs:documentation>
                                <usage>all</usage>
                            </xs:documentation>
