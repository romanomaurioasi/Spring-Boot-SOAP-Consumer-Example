    <!-- Particulars -->
    <xs:complexType name="ParticularsBaseType">
        <xs:sequence>
            <xs:element name="Social-InsuranceIdentification"
                        type="c:Social-InsuranceIdentificationType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Sozialversicherungsidentifikation</translation>
                        <description>
                            <short>Von der Sozialversicherung zugeteilte
                                Soziallversicherungsidentifikation (eindeutige Identifikation von
                                Versicherten).
                            </short>
                            <domain>In der Übergangszeit sind möglichst alte und neue
                                Versichertennummer zu liefern.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identification d'assurance sociale</translation>
                        <description>
                            <short>Identification d'assurance sociale qui est attribué par
                                l'assurance sociale (identification unique des assurés).
                            </short>
                            <domain>Pendant la période de transition il faudra indiquer de
                                préférence l'ancien ainsi que le nouveau numéro d'assuré.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="EmployeeNumber" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Personalnummer</translation>
                        <description>
                            <short>Personalnummer der Person (frei nach den Bedürfnissen des
                                Unternehmens)
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro personnel</translation>
                        <description>
                            <short>Numéro personnel de la personne (selon les besoins de
                                l'entreprise)
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Lastname" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Nachname</translation>
                        <description>
                            <short>Vollständiger Familienname der Person</short>
                            <domain>Name gemäss amtlichem Papier (z.B. Versicherungsnachweis)
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nom</translation>
                        <description>
                            <short>Nom complet de la personne</short>
                            <domain>Nom selon papier officiel (p.e. attestation d'assurance)
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Firstname" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Vorname</translation>
                        <description>
                            <short>Vorname(n) der Person</short>
                            <domain>Vorname gemäss amtlichem Papier (z.B. Versicherungsnachweis)
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Prénom</translation>
                        <description>
                            <short>Prénom(s) de la personne</short>
                            <domain>Prénom selon papier officiel (p.e. attestation d'assurance)
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Sex" type="c:SexType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Geschlecht</translation>
                        <description>
                            <short>Geschlecht der Person</short>
                            <domain>Die Unterscheidung zwischen männlichen und weiblichen
                                Mitarbeitern ist notwendig, weil das unterschiedliche
                                AHV-Rentenalter Einfluss auf die Berechnung des AHV-Freibetrages
                                hat. Ausserdem werden vom Arbeitgeber statistische Angaben über die
                                Anzahl Männer und Frauen verlangt.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Sexe</translation>
                        <description>
                            <short>Sexe de la personne</short>
                            <domain>La distinction entre les employés masculins et féminins est
                                nécessaire à cause de l'âge de la retraite différent qui influence
                                le calcul du montant exonéré des cotisations AVS. En outre
                                l'employeur doit fournir des indications statistiques sur le nombre
                                d'hommes et de femmes employés.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DateOfBirth" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Geburtsdatum</translation>
                        <description>
                            <short>Geburtsdatum der Person</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date de naissance</translation>
                        <description>
                            <short>Date de naissance de la personne</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Nationality" type="c:NationalityType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Nationalität</translation>
                        <description>
                            <short>Nationalität 2-stellig nach ISO 3166 Erweitert</short>
                            <technical>Erweiterung 11= unbekannt 22= staatenlos : Die ISO-Codes
                                wurden auf dem Prod-Distri noch nie geprüft: In der Qualitätsstufe
                                Plausibilität kann evtl. gegen die ISO 3166 Codes geprüft und eine
                                Warnung angezeigt werden.
                                http://www.unece.org/etrades/unedocs/repository/codelists/xml/CountryCode.xsd
                                Achtung: Probleme mit bestehenden Datenbeständen (Änderungen der
                                Ländernamen und Verwechslungen)
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nationalité</translation>
                        <description>
                            <short>Nationalité en code de deux chiffres selon ISO 3166 élargi</short>
                            <technical>Élargissement 11=inconnu 22=apatride : Les codes ISO n'ont jamais été vérifiés
                                sur le Prod-Distri auparavant.: Dans le contrôle de qualité,
                                niveau plausibilité, le document est testé contre les codes ISO 3166
                                et un avertissement est généré au cas d'un code inconnu.
                                http://www.unece.org/etrades/unedocs/repository/codelists/xml/CountryCode.xsd
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CivilStatus" type="c:CivilStatusAndDateType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Zivilstandsangaben</translation>
                        <description>
                            <short>Zivilstand der Person und gültig ab Datum</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>État civil</translation>
                        <description>
                            <short>État civil de la personne</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
