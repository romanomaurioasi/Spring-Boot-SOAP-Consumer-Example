    <!--  IncidentDeclarationRequest
    -->
    <xs:complexType name="DeclareIncidentRequestType">
        <xs:sequence>
            <xs:element name="RequestContext" type="idc:RequestContextType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsKontext</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contexte de la demande</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Job" type="idc:JobType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Arbeitsauftrag</translation>
                        <description>
                            <short>
                                Arbeitsauftrag für sämtliche Empfänger (Distributor und Institutionen)
                            </short>
                            <technical>
                                Hier sind alle Empfänger aufgeführt
                                - Distributor
                                - Institutions-Empfänger der Domänen
                                - carbon copy Treuhänder und Broker
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Commande</translation>
                        <description>
                            <short>
                                Commande pour tous les receveurs (distributeur et institutions)
                            </short>
                            <technical>
                                Liste de tous les receveurs
                                - distributeur
                                - receveurs des institutions des domaines
                                - receveurs "carbon copy" : agents fiduciaires, courtiers
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IncidentDeclaration" type="id:IncidentDeclarationType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Ereignismeldung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Déclaration de l’événement</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

