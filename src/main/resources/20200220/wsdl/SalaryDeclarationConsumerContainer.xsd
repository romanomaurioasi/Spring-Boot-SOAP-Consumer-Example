    <xs:complexType name="DistributorRequestContextType">
        <xs:sequence>
            <xs:element name="UserAgent" type="c:UserAgentType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Applikationsinformation</translation>
                        <description>
                            <short>Beschreibung der wesentlichen System-Identifikations-Daten</short>
                            <technical>
                                Zur Qualitätsicherung des Übermittlungsprozesses werden die wesentlichen
                                Informationen der beteiligten gesichert
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Informations concernant l'application</translation>
                        <description>
                            <short>Description des données essentielles d'identification de système</short>
                            <technical>
                                Pour l'assurance de la qualité, les informations essentielles des participants
                                sont sauvegardées.
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Informazioni sull'applicazione</translation>
                        <description>
                            <short>Descrizione dei dati essenziali per l'identificazione del sistema</short>
                            <technical>
                                Per garantire la qualità, le informazioni essenziali dei partecipanti
                                sono salvati.
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransmissionDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Übertragungszeitpunkt</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date et Temps de transmission</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Data e ora di trasmissione</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeclarationID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Gleiche ID in allen Requests (Distributor -> End-Empfänger),
                                Responses (Distributor -> Transmitter), Masken und Pdf‘s, welche
                                zum identischen Geschäftsvorfall gehören. Damit kann sie als
                                Fallnummer für den Support verwendet werden.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Identificateur qui reste le même dans toutes les requêtes
                                (Distributeur -> End-Empfänger), réponses (DistributeuràTransmetteur),
                                masques et Pdf‘s faisant partie d‘une opération commerciale identique.
                                Il peut être utilisé comme numéro de cas pour le support.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Identificatore che rimane lo stesso in tutte le domande
                                (Distributore -> End-Empfänger), risposte (dal distributore al trasmettitore),
                                maschere e Pdf fanno parte di un'identica operazione commerciale.
                                Può essere utilizzato come numero di caso per il supporto.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DistributorRequestID" type="c:IDType"/>
            <xs:element name="TestCase" type="c:EmptyType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Testfall</translation>
                        <description>
                            <short>Testfall-Kennzeichnung</short>
                            <technical>Die Daten wurden empfangen und quittiert. Aber OHNE
                                PRODUKTIVE VERARBEITUNG!!!
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Cas de test</translation>
                        <description>
                            <short>Marquage pour cas de test</short>
                            <technical>Les données sont reçus et acquittés. Mais SANS
                                TRAITEMENT PRODUCTIVE!!!
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Casi di test</translation>
                        <description>
                            <short>Marcatura per i casi di test</short>
                            <technical>I dati vengono ricevuti e riconosciuti. Ma SENZA
                                TRATTAMENTO PRODUTTIVO!!!!
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Duplicate" type="sdcc:DuplicateType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Duplikat</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Dupliquer</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Duplicare</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="SubstitutionMapping" type="sdcc:SubstitutionMappingType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Substitutions-Zuordnung</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Substitution Affectation</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Sostituzione Assegnazione</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="VersionMappingFrom" type="sdcc:VersionMappingFromType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Versionsabbildung von</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Version Mapping de</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Versione Mappatura Da</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ProducerSecurityTokens" type="sdcc:ProducerSecurityTokensType">
                <xs:unique name="X509CertificateID">
                    <xs:selector xpath="sdcc:X509Certificate"/>
                    <xs:field xpath="@X509CertificateID"/>
                </xs:unique>
            </xs:element>
            <xs:element name="SignatureCertificateUID-BFS" type="sdcc:SignatureCertificateUID-BFSType" minOccurs="0"/>
            <xs:element name="ProducerResponseNotifications" type="sdcc:ProducerResponseNotificationsType"/>
            <xs:element name="ConsumerNotifications" type="sdcc:ConsumerNotificationsType" minOccurs="0"/>
            <xs:element name="Institutions" type="sd:CustomerIdentificationType" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SignatureCertificateUID-BFSType">
        <xs:sequence>
            <xs:element name="CommonName" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Common Name</translation>
                        <description>
                            <technical>
                                NTRCH-{UID-BFS}@swissdec.ch
                                mit Beispiel:
                                NTRCH-CHE-123.456.789@swissdec.ch
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Common Name</translation>
                        <description>
                            <technical>
                                NTRCH-{UID-BFS}@swissdec.ch
                                à titre d'exemple:
                                NTRCH-CHE-123.456.789@swissdec.ch
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Common Name</translation>
                        <description>
                            <technical>
                                NTRCH-{UID-BFS}@swissdec.ch
                                a titolo di esempio:
                                NTRCH-CHE-123.456.789@swissdec.ch
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="UID" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>UID</translation>
                        <description>
                            <technical>
                                UID Nr. nach BFS UID-Register
                                OID Object Identifier (2.5.4.97)
                                mit Beispiel
                                NTRCH-CHE-123.456.789
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>UID</translation>
                        <description>
                            <technical>
                                UID Nr. selon le registre UID du OFS
                                OID Object Identifier (2.5.4.97)
                                mit Beispiel
                                NTRCH-CHE-123.456.789
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>UID</translation>
                        <description>
                            <technical>
                                UID Nr. secondo il registro UID UST
                                OID Object Identifier (2.5.4.97)
                                mit Beispiel
                                NTRCH-CHE-123.456.789
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>all</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="X509CertificateIDRef" type="c:InstanceRefIDType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>X509Certificate Identifikation Referenz</translation>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Référence X509Certificate Identificateur</translation>
                </xs:documentation>
                <xs:documentation xml:lang="it">
                    <translation>Riferimento X509Certificate Identificazione</translation>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>


    <xs:complexType name="DuplicateType">
        <xs:sequence>
            <xs:element name="LastDistributorRequestID" type="c:IDType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="SubstitutionMappingType">
        <xs:sequence>
            <xs:element name="MappedPredecessorDistributorRequestID" type="c:IDType"/>
            <xs:element name="MappedPredecessorConsumerResponseID" type="c:IDType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ProducerSecurityTokensType">
        <xs:sequence>
            <xs:element name="Timestamp" type="xs:dateTime" maxOccurs="unbounded"/>
            <xs:element name="X509Certificate" type="sdcc:X509CertificateType"
                        maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="X509CertificateType">
        <xs:sequence>
            <xs:element name="Usage" type="sdcc:UsageType"/>
            <xs:element name="DistinguishedNameOwner" type="xs:string"/>
            <xs:element name="DistinguishedNameIssuer" type="xs:string"/>
            <xs:element name="SecuredElement" type="xs:string" maxOccurs="unbounded"/>
            <xs:element name="PEM" type="xs:base64Binary"/>
        </xs:sequence>
        <xs:attribute name="X509CertificateID" type="c:InstanceRefIDType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>X509Certificate Identifikation</translation>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Identificateur X509Certificate</translation>
                </xs:documentation>
                <xs:documentation xml:lang="it">
                    <translation>Identificazione X509Certificate</translation>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="UsageType">
        <xs:restriction base="xs:NMTOKEN">
            <xs:enumeration value="Signature"/>
            <xs:enumeration value="QualifiedSignature"/>
            <xs:enumeration value="Encryption"/>
        </xs:restriction>
    </xs:simpleType>

