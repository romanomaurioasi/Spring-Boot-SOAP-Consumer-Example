                    <xs:selector xpath="sdc:SalaryDeclaration/sd:Company/sd:Institutions/sd:Statistic"/>
                    <xs:field xpath="@institutionID"/>
                </xs:unique>
                <xs:keyref name="Statistic-Ref" refer="sdcc:Statistic-Key">
