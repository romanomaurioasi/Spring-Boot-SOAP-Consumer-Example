    <xs:complexType name="ReducedJobStateType">
        <xs:choice>
            <xs:element name="AHV-AVS" type="sdc:ReducedWorkingStateMultiType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AHV</translation>
                    </xs:documentation>
