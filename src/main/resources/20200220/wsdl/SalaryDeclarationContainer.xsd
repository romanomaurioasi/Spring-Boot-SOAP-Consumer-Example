    <xs:complexType name="SubstitutionType">
        <xs:sequence>
            <xs:element name="PredecessorDeclarationIDWithAcceptedState" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>MeldungsID</translation>
                        <description>
                            <short>
                                Die MeldungsID wird vom Distributor vergeben und verwaltet.
                                Damit lassen sich mehrere Prozess-Schritte "verknüpfen" und
                                ergibt dadurch eine Fall- oder Supportnummer.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identification du message</translation>
                        <description>
                            <short>
                                L'identification du message (MeldungsID) est donnée et administrée
                                par le distibuteur. Avec "MeldungsID", plusieurs pas de processus
                                peuvent être enchaîné. Il résulte un numéro de cas ou de support.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Identificazione del messaggio</translation>
                        <description>
                            <short>
                                L'identificazione del messaggio (MeldungsID) viene data e gestita
                                dal distibutor. Con "MeldungsID", diverse fasi del processo
                                possono essere incatenati insieme. Il risultato è un numero di caso o di media.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!-- response of the operations getStatus... and getResult...
    -->
    <xs:complexType name="ResponseContextType">
        <xs:sequence>
            <xs:element name="UserAgent" type="c:UserAgentType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Applikationsinformation</translation>
                        <description>
                            <short>
                                Beschreibung der wesentlichen System-Identifikations-Daten
                            </short>
                            <technical>
                                Zur Qualitätsicherung des Übermittlungsprozesses werden die wesentlichen Informationen
                                der beteiligten gesichert
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Informations concernant l'application</translation>
                        <description>
                            <short>
                                Description des données essentielles d'identification de système
                            </short>
                            <technical>
                                Pour l'assurance de la qualité, les informations essentielles des participants
                                sont sauvegardées.
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Informazioni sull'applicazione</translation>
                        <description>
                            <short>
                                Descrizione dei dati essenziali per l'identificazione del sistema
                            </short>
                            <technical>
                                Per garantire la qualità, le informazioni essenziali dei partecipanti
                                sono salvati.
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InstitutionName" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Institutionsname</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nom de l'institution</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Nome dell'istituzione</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TransmissionDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Übertragungszeitpunkt</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date et Temps de transmission</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Data e ora di trasmissione</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DeclarationID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Gleiche ID in allen Requests (Distributor -> End-Empfänger),
                                Responses (Distributor -> Transmitter), Masken und Pdf‘s, welche
                                zum identischen Geschäftsvorfall gehören. Damit kann sie als
                                Fallnummer für den Support verwendet werden.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Identificateur qui reste le même dans toutes les requêtes
                                (Distributeur -> End-Empfänger), réponses (DistributeuràTransmetteur),
                                masques et Pdf‘s faisant partie d‘une opération commerciale identique.
                                Il peut être utilisé comme numéro de cas pour le support.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>DeclarationID</translation>
                        <description>
                            <short>Identificatore che rimane lo stesso in tutte le domande
                                (Distributore -> End-Empfänger), risposte (dal distributore al trasmettitore),
                                maschere e Pdf fanno parte di un'identica operazione commerciale.
                                Può essere utilizzato come numero di caso per il supporto.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ResponseID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AntwortID</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de la réponse</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Identificazione della risposta</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="RequestID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>AuftragsID</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de la demande</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="it">
                        <translation>Identificatore di applicazione</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TestCase" type="c:EmptyType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Testfall</translation>
                        <description>
                            <short>Testfall-Kennzeichnung</short>
                            <technical>Die Daten wurden empfangen und quittiert. Aber OHNE
                                PRODUKTIVE VERARBEITUNG!!!
                            </technical>
