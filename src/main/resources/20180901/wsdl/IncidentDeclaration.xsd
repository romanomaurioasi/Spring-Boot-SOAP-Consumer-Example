                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Percentage" type="c:PercentType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Prozent</translation>
                        <description>
                            <short>
                                Prozentsatz, um den die Leistung gekürzt wird.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>pourcentage</translation>
                        <description>
                            <short>
                                Pourcentage de réduction du service.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CoveringDays" type="id:DaysType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Bezugstage</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Journées d’indemnités</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="QuantityOfDays" type="xs:short" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Anzahl Tage</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Nombre de jours</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Period" type="c:TimePeriodType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Periode</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Période</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReductionAllowance" type="c:SalaryAmountAbsoluteType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Kürzungsansatz</translation>
                        <description>
                            <short>Betrag der vom Versicherer gemachten Kürzung pro Tag. Angabe in CHF
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>approche de réduction</translation>
                        <description>
                            <short>Montant de la réduction effectuée par l'assureur par jour. En CHF</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ReductionAmountFavourInstitution" type="c:SalaryAmountAbsoluteType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Kürzungsbetrag zu Gunsten Versicherer</translation>
                        <description>
                            <short>
                                Kürzungen sind immer eine Herabsetzung der Leistungen. Daher ist der totale Betrag der
                                Kürzung immer zu Gunsten des Versicherers. Angabe in CHF.
                            </short>
                            <domain/>
                            <technical/>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Montant de la réduction en faveur de l'assureur</translation>
                        <description>
                            <short>
                                Les réductions sont toujours une réduction des prestations. Par conséquent, le montant
                                total de la réduction est toujours en faveur de l'assureur. Indication en CHF.
                            </short>
                            <domain/>
                            <technical/>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="AmountBeforeReductionFavourCompany" type="c:SalaryAmountAbsoluteType"
