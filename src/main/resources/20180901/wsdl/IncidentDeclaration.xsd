    <!-- IncapacitiesToWork -->
    <xs:complexType name="IncapacitiesToWorkType">
        <xs:complexContent>
            <xs:extension base="id:StoryBaseType">
                <xs:choice>
                    <xs:element name="IncapacityToWork" type="id:IncapacityToWorkType" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Arbeitsunfähigkeit</translation>
                                <description>
                                    <short>
                                        Arbeitsunfähigkeit
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>incapacité de travail</translation>
                                <description>
                                    <short>
                                        incapacité de travail
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
