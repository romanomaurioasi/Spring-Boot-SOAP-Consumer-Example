                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Identificateur de contrat</translation>
                        <description>
                            <short>Identificateur de contrat attribuée par l'assurance.</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InsuranceCode" type="c:AssuranceCategoryCodeType" minOccurs="0"
                        maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
    <!-- PaymentLink story -->
    <xs:complexType name="RepaymentType">
        <xs:complexContent>
