    <xs:complexType name="RegistrationResponseType">
        <xs:sequence>
            <xs:element name="ResponseState" type="id:ResponseStateType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Antwort-Status</translation>
                        <description>
                            <short>Information ob die Deklaration für die Person erfolgreich war oder nicht.</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>État de la réponse</translation>
                        <description>
                            <short>Information indiquant si la déclaration pour la personne a été effectuée avec succès
                                ou non.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="CompanyCaseID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Unternehmen Fall ID</translation>
                        <description>
                            <short>Schlüsselwerte zum Ereignis</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro d'identification du cas de l'entreprise</translation>
                        <description>
                            <short>
                                Valeurs clés relatives à l’événement
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="InsuranceCaseID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Versicherer Fall ID</translation>
                        <description>
                            <short>
                                Fall ID von dem Versicherersystem
                                (alte Bezeichnung: Schadennummer)
                            </short>
                            <technical>
                                Achtung:
                                Diese Versicherer Fall ID kann im Prozessverlauf geändert werden!
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro d'identification du cas de l'assureur</translation>
                        <description>
                            <short>
                                Numéro d'identification du cas du système de l'assureur
                                (ancienne désignation : numéro de déclaration de perte)
                            </short>
                            <technical>Attention : Ce numéro de dossier d'assureur peut être modifié pendant le
                                processus!
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="IncidentCaseID" type="c:IDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>IncidentCaseID</translation>
                        <description>
                            <short>
                                Die IncidentCaseID wird vom Distributor vergeben und verwaltet.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>IncidentCaseID</translation>
                        <description>
                            <short>
                                Le IncidentCaseID est attribué et géré par le distributeur.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ContactInsurance" type="c:ContactInstitutionType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Kontakt Versicherer</translation>
                        <description>
                            <short>
                                Kontaktangaben für das Unternehmen bei Rückfragen zur Abrechnung des
                                Versiche-rers.
                                z.B.
                                Name:
                                Team Abrechnung
                                EmailAddress:
                                abrechnung@company.ch
                                PhoneNumber:
                                041 851 51 52
                                MobilePhoneNumber
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Contact assureur</translation>
                        <description>
                            <short>
                                Données de contact pour l’entreprise en cas de questions sur le décompte de
                                l’assureur,
                                z.B.
                                Nom:
                                Team Décompte
                                EmailAddress:
                                abrechnung@company.ch
                                PhoneNumber:
                                041 851 51 52
                                MobilePhoneNumber
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TestCase" type="c:EmptyType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Testfall</translation>
                        <description>
                            <short>
                                Testfall-Kennzeichnung
                            </short>
                            <technical>
                                Die Daten sollen empfangen und quittiert werden.
                                Aber OHNE PRODUKTIVE VERARBEITUNG!!!
                            </technical>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Cas de test</translation>
                        <description>
                            <short>
                                Identificateur pour des cas de test
                            </short>
                            <technical>
                                Reception et quittance des données, mais
                                SANS TRAITEMENT PRODUCTIF!!!
                            </technical>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="incidentCaseIDMapRef" type="c:InstanceRefIDType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>IncidentCaseID Abbildungs Referenz</translation>
                    <description>
                        <short>
                            Hier wird die Referenz aus dem Request gespiegelt.
                        </short>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Référence IncidentCaseID</translation>
                    <description>
                        <short>
                            La référence de la demande est reproduite ici.
                        </short>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <xs:complexType name="AccidentRegistrationResponseType">
        <xs:complexContent>
            <xs:extension base="id:RegistrationResponseType">
                <xs:sequence>
                    <xs:element name="DeliverCustomerIdentity" type="c:NotEmptyStringType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Versicherungs Kunden Identifikation temporär</translation>
                                <description>
                                    <short>
                                        Antwort des Versicherers auf eine Registrierung ohne Vertrag.
                                    </short>
                                    <domain/>
                                    <technical/>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Assurance Clients Identification temporaire</translation>
                                <description>
                                    <short>
                                        Réponse de l’assureur à un enregistrement sans contrat.
                                    </short>
                                    <domain/>
                                    <technical/>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="DeliverContractIdentity" type="c:NotEmptyStringType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Versicherungs Kunden Identifikation temporär</translation>
                                <description>
                                    <short>
                                        Antwort des Versicherers auf eine Registrierung ohne Vertrag.
                                    </short>
                                    <domain/>
                                    <technical/>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Assurance Clients Identification temporaire</translation>
                                <description>
                                    <short>
                                        Réponse de l’assureur à un enregistrement sans contrat.
                                    </short>
                                    <domain/>
                                    <technical/>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
