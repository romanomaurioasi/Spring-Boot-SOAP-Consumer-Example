    <!-- ProcessCtrl -->
    <xs:complexType name="ProcessCtrlResponseType">
        <xs:complexContent>
            <xs:extension base="id:StoryBaseType">
                <xs:sequence>
                    <xs:element name="ChangeInsuranceCaseID" type="id:OldNewIDType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Wechsel InsuranceCaseID</translation>
                                <description>
                                    <short>
                                        Die InsuranceCaseID ist ein verän-derbarer Schlüssel im Versicherer-system. Wird
                                        die InsuranceCaseID vom Versicherer geändert, muss das Unternehmen informiert
                                        wer-den. Dazu werden einerseits die «alte», bisher gültige InsuranceCa-seID
                                        sowie die «neu» gültige ge-meldet.
                                        Ab diesem Zeitpunkt muss das ERP-System nur noch die neue InsuranceCaseID
                                        verwenden (Syn-chronisierung, Papierausdrucke usw.).
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Modifier l'InsuranceCaseID</translation>
                                <description>
                                    <short>
                                        Le InsuranceCaseID est une clé modifiable dans le système d'assurance. Si
                                        l'InsuranceCaseID est modifié par l'assureur, la compagnie doit en être
                                        informée. L'"ancienne" InsuranceCa-seID et la "nouvelle" InsuranceCa-seID
                                        valides sont déclarées à cet effet.

                                        A partir de ce moment, le système ERP n'a plus qu'à utiliser le nouveau
                                        InsuranceCaseID (synchronisation, impressions papier, etc.).
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="InstitutionAddress" type="c:NameAndAddressType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Adresse Versicherer</translation>
                                <description>
                                    <short>
                                        Der Versicherer kann dem Unter-nehmen seine für das Ereignis zuständige Adresse
                                        mitteilen (z. B. Geschäftsstelle). Dabei kann es sich um eine Erstmeldung oder
                                        bei erneuter Übermittlung um eine Änderung handeln.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Adresse de l'assureur</translation>
                                <description>
                                    <short>
                                        L'assureur peut communiquer à l'entreprise l'adresse responsable de l'événement
                                        (p. ex. succursale). Il peut s'agir d'une notification initiale ou, dans le cas
                                        d'une retransmission, d'une modification.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ContactInsurance" type="c:ContactInstitutionType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Kontakt Versicherer</translation>
                                <description>
                                    <short>
                                        Der Versicherer kann dem Unter-nehmen seine für das Ereignis zuständige Adresse
                                        mitteilen (z. B. Geschäftsstelle). Dabei kann es sich um eine Erstmeldung oder
                                        bei erneuter Übermittlung um eine Änderung handeln.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Contacter l'assureur</translation>
                                <description>
                                    <short>
                                        L'assureur peut communiquer à l'entreprise l'adresse responsable de l'événement
                                        (p. ex. succursale). Il peut s'agir d'une notification initiale ou, dans le cas
                                        d'une retransmission, d'une modification.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="AwaitStory" type="id:AwaitPartType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Part anfordern</translation>
                                <description>
                                    <short>Einen Part anfordern</short>
                                    <technical>Achtung für den Endempfänger: Vor dem Abholen
                                        (Response absetzen) eines AwaitPart muss im aktuellen
                                        Request geprüft werden, ob ein angeforderter Part bereits im
                                        Request war. In so einem Fall darf der AwaitPart nicht mehr
                                        verlangt werden.
                                    </technical>
                                </description>
                            </xs:documentation>
                            <xs:documentation>
                                <usage>all</usage>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Comment" type="c:NotificationsType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Bemerkungen</translation>
                                <description>
                                    <short>Die Bemerkungen sind zurzeit für die Pilotphase gedacht.
                                        Damit erhöhen wir die Flexibilität und ermöglichen einfache
                                        Korrekturen
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Remarques</translation>
                                <description>
                                    <short>
                                        Les commentaires sont actuellement destinés à la phase pilote. Cela augmente
                                        la flexibilité et permet des corrections simples.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
