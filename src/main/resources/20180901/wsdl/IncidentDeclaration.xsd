                        <translation>Informations</translation>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="Warning" type="c:NotificationsType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Warnungen</translation>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
