                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Code postal (CP)</translation>
                        <description>
                            <short>Code postal (5e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="City" type="xs:string">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Ort</translation>
                        <description>
                            <short>Ort (5. Zeile)</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Lieu</translation>
                        <description>
                            <short>Lieu (5e ligne)</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!-- Postaddressee line 6 -->
            <xs:element name="Country" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Land</translation>
                        <description>
                            <short>Land für Postversand (6. Zeile)</short>
