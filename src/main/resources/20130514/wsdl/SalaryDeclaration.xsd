                                <translation>Cotisation du salarié</translation>
                                <description>
                                    <short>Cotisations d'une personne assuré qui sont cotisés par les retenues
                                        sur le salaire.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="EmployerContribution" type="sd:SalaryAmountType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Arbeitgeberbeitrag</translation>
                                <description>
                                    <short>Teil der Kosten, die dem Arbeitgeber erwachsen.</short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Cotisation de l'employeur</translation>
                                <description>
                                    <short>Partie des frais pour l'employeur.</short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ThirdPartyContribution" type="sd:SalaryAmountType"
                                minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Beiträge von Dritten</translation>
                                <description>
                                    <short>BVG-Beiträge von einem weiteren Beitragszahler
                                        (z.B. Fonds der Vorsorgeeinrichtung)
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Cotisation par des tiers</translation>
                                <description>
                                    <short>Cotisations LPP qui sont payées par des cotisants supplémentaires
                                        (p.e. fonds de prévoyance)
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
                <xs:choice>
                    <xs:element name="Unknown" type="sd:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>unbekannt</translation>
                                <description>
                                    <short>
                                        Der übermittelte BVG-Code ist beim Versicherer nicht bekannt.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>inconnu</translation>
                                <description>
                                    <short>
                                        Le code LPP transmis n'est pas connu par l'assureur.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="Missing" type="sd:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>fehlend</translation>
                                <description>
                                    <short>
                                        Der BVG-Code wurde nicht übermittelt und fehlt deshalb.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>manquant</translation>
                                <description>
                                    <short>
                                        Le code LPP n'était pas transmis et manque à cause de ça.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="NoContributionsAvailable" type="sd:EmptyType">
                        <xs:annotation>
                            <xs:documentation xml:lang="de">
                                <translation>Beitrag nicht verfügbar</translation>
                                <description>
                                    <short>
                                        Der BVG-Beitrag ist nicht verfügbar, da er aus unbekannten Gründen nicht
                                        berechnet werden konnte.
                                    </short>
                                </description>
                            </xs:documentation>
                            <xs:documentation xml:lang="fr">
                                <translation>Cotisation LPP n'est pas disponible</translation>
                                <description>
                                    <short>
                                        La cotisation LPP n'est pas disponible parsequ'elle ne peut pas être
                                        calculé en raison inconnue.
                                    </short>
                                </description>
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:choice>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>

