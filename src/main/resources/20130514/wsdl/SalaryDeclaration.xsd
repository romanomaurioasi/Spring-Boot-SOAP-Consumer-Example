            </xs:element>
            <xs:element name="Postbox" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Postfach</translation>
                        <description>
                            <short>Postfach (4. Zeile)</short>
                        </description>
                    </xs:documentation>
