    <xs:complexType name="TaxAtSourceCorrectionType">
        <xs:sequence>
            <xs:element name="Month" type="xs:gYearMonth">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Jahr und Monat</translation>
                        <description>
                            <short>Jahr und Monat der korrigierten QST-Abrechnung</short>
                            <domain>
                                Sollte die Korrekturperiode mehr als einen Monat ausmachen,
                                muss diese Korrektur auf die entsprechenden Monate aufgeteilt werden.
                                D.h. es werden nur einzelne Monate (Monatspakete) übermittelt.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Année et mois</translation>
                        <description>
                            <short>Année et mois de déclaration IS corrigé</short>
                            <domain>
                                Si la période de correction comporte plusieurs mois,
                                cette correction doit être divisé sur les mois correspondants.
                                C'est à dire seulement les individuels mois(paquets mensuels) sont transmis.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>TAtSrc</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:choice>
                <xs:element name="Old" type="sd:TaxAtSourceCorrectionOldType">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">
                            <translation>Korrektur Monat alt</translation>
                            <description>
                                <short>Korrektur Monat alt</short>
                                <domain>
                                    Rückwirkende Korrektur (Storno) einer Quellensteuerabrechnung
                                </domain>
                            </description>
                        </xs:documentation>
                        <xs:documentation xml:lang="fr">
                            <translation>Correction mois vieux</translation>
                            <description>
                                <short>Correction mois vieux</short>
                                <domain>
                                    Correction rétroactive (rectification) d'une déclaration impôt à la source
                                </domain>
                            </description>
                        </xs:documentation>
                        <xs:documentation>
                            <usage>TAtSrc</usage>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="Difference" type="sd:TaxAtSourceCorrectionDifferenceType">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">
                            <translation>Differenz des Monats</translation>
                            <description>
                                <short>Differenz des Monats</short>
                                <domain>
                                    In der Lohnbuchhaltung ermittelte Differenz einer Quellensteuerabrechnung
                                </domain>
                            </description>
                        </xs:documentation>
                        <xs:documentation xml:lang="fr">
                            <translation>Différence du mois</translation>
                        </xs:documentation>
                        <xs:documentation>
                            <usage>TAtSrc</usage>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
            <xs:element name="New" type="sd:TaxAtSourceCorrectionNewType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Korrektur Monat neu</translation>
                        <description>
                            <short>Korrektur Monat neu</short>
                            <domain>
                                Rückwirkende Korrektur (Neu-Berechnung) einer Quellensteuerabrechnung
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Correction mois nouveau</translation>
                        <description>
                            <short>Correction mois nouveau</short>
                            <domain>
                                Correction rétroactive (recalcul) d'une déclaration impôt à la source
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>TAtSrc</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CorrectionConfirmedType">
        <xs:sequence>
            <xs:element name="Month" type="xs:gYearMonth">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Jahr und Monat</translation>
                        <description>
                            <short>Jahr und Monat der korrigierten QST-Abrechnung</short>
                            <domain>
                                Wenn Korrekturen für mehrere Monate bestätigt werden,
                                sollten diese auf die entsprechenden Monate aufgeteilt werden.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Année et mois</translation>
                        <description>
                            <short>Année et mois de déclaration IS corrigé</short>
                            <domain>
                                Si on confirme corrections pour plusieurs mois
                                les corrections doivent être attribués aux mois correspondent
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>TAtSrc</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxableEarning" type="sd:SalaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>QST-Lohn</translation>
                        <description>
                            <short>QST-Lohn</short>
                            <domain>
                                In der Lohnbuchhaltung korrigierte Differenz, die der KSTV bestätigt wird.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Prestations soumises à l’impôt</translation>
                        <description>
                            <short>Prestations soumises à l’impôt</short>
                            <domain>
                                La difference corrigée dans la comptabilité salariale
                                et confirmé au l'ACI
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation>
                        <usage>TAtSrc</usage>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="TaxAtSource" type="sd:SalaryAmountType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>QST-Betrag</translation>
                        <description>
