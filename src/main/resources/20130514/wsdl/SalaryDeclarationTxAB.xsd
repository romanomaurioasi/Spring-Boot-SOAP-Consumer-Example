<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:sdtab="http://www.swissdec.ch/schema/sd/20130514/SalaryDeclarationTxAB"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="http://www.swissdec.ch/schema/sd/20130514/SalaryDeclarationTxAB"
           elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.0">

    <!-- state of standardisation: FINAL
         publication: 2013-05-21
         a. boehm
    -->


    <!--
         Include -->
    <xs:include schemaLocation="20130514SalaryDeclaration_Tax_noNS.xsd"/>
    <!--

        elements -->
    <xs:element name="T" type="sdtab:SalaryDeclarationTaxAccBcdType"/>
    <!--

        SalaryDeclaration Tax with 2dBarcode
    -->
    <xs:complexType name="SalaryDeclarationTaxAccBcdType">
        <xs:sequence>
            <!--
                company data -->
            <xs:element name="Company" type="sdtab:CompanyIDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Unternehmen</translation>
                        <description>
                            <short>Unternehmenbeschreibung</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Entreprise</translation>
                        <description>
                            <short>Description d'entreprise</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!--
                person data -->
            <xs:element name="PersonID" type="sdtab:PersonIDType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>PersonID</translation>
                        <description>
                            <short>Identifikation der Person</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>PersonID</translation>
                        <description>
                            <short>Identification de la Person</short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <!--
                salary data -->
            <xs:choice>
                <!--
                    text-recombine
                    with namespace sd:http://www.swissdec.ch/schema/sd/20051002/SalaryDeclaration -->
                <xs:element name="A" type="sdtab:TaxAnnuityType">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">
                            <translation>Steuerbare Rente (B)</translation>
                            <description>
                                <short>Rente gemäss Rentenbescheinigung</short>
                            </description>
                        </xs:documentation>
                        <xs:documentation xml:lang="fr">
                            <translation>Rente imposable (B)</translation>
                            <description>
                                <short>Rente selon l'attestation de rentes</short>
                            </description>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="S" type="sdtab:TaxSalaryType">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">
                            <translation>Steuerbares Einkommen (A)</translation>
                            <description>
                                <short>Einkommen gemäss Lohnausweis</short>
                            </description>
                        </xs:documentation>
                        <xs:documentation xml:lang="fr">
                            <translation>Revenu imposable (A)</translation>
                            <description>
                                <short>Revenu selon le certificat de salaire</short>
                            </description>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
                <!--
                    reduced definition in tns namespace  -->
                <xs:element name="R" type="sdtab:ReducedTaxSalaryType">
                    <xs:annotation>
                        <xs:documentation xml:lang="de">
                            <translation>Steuerbares Einkommen (A) Datenvolumen reduziert</translation>
                            <description>
                                <short>Einkommen gemäss Lohnausweis für 2DBarcode reduziert</short>
                                <technical>Die reduzierte Struktur wird in den Fällen verwendet, in welchen die
                                    Datenmenge auch nach dem Komprimieren für den 2DBarcode immer noch zu gross ist.
                                    PDF417 ISO/IEC 15438 Standard erlaubt 1108 Bytes zu codieren.
                                </technical>
                            </description>
                        </xs:documentation>
                        <xs:documentation xml:lang="fr">
                            <translation>Revenu imposable (A) Volume de données réduit</translation>
                            <description>
                                <short>Revenu selon le certificat de salaire réduit du au code
                                    barres
                                </short>
                                <technical>La structure réduite est utilisée dans les cas où la
                                    quantité de données est trop grande pour le code barres 2D même
                                    après avoir été comprimée. Le standard PDF417 ISO/IEC 15438
                                    permet de coder au plus 1108 Bytes.
                                </technical>
                            </description>
                        </xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:choice>
        </xs:sequence>
        <xs:attribute name="SID" type="sdtab:SIDType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>SystemID</translation>
                    <description>
                        <short>SystemID, Kennzeichen des Herstellers</short>
                        <technical>Die SystemID wird während der Zertifizierung definiert und durch swissdec
                            verwaltet und publiziert.
                        </technical>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>SystemID</translation>
                    <description>
                        <short>SystemID, Identification du développeur</short>
                        <technical>Le SystemID est défini pendant la certification. Il est
                            administré et publié par swissdec.
                        </technical>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="SysV" type="sdtab:SysVType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>SystemVersion</translation>
                    <description>
                        <short>SystemVersion</short>
                        <technical>Die SystemVersion wird durch den Hersteller gepflegt. In Problemfällen wird
                            damit die Suche nach der Ursache eingegrenzt.
                        </technical>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>SystemVersion</translation>
                    <description>
                        <short>SystemVersion</short>
                        <technical>SystemVersion est administré par le développeur. Au cas de
                            problèmes, la vérsion aide à analyser la cause de l'erreur.
                        </technical>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!--
        CompanyType -->
    <xs:complexType name="CompanyIDType">
        <xs:attribute name="UID-BFS" type="sdtab:UID-BFSType" use="optional">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>Unternehmensidentifikation</translation>
                    <description>
                        <short>Unternehmensidentifikation nach BFS
                        </short>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Identification de l'entreprise</translation>
                    <description>
                        <short>Identification de l'entreprise selon l’OFS
                        </short>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="HR-RC-Name" type="sdtab:HR-RC-NameType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>Name oder HR-Name</translation>
                    <description>
                        <short>Name oder HR-Name des Unternehmens</short>
                        <domain>Falls eine eingetragener Handelsregisternamen vorhanden ist, muss dieser Name
                            verwendet werden.
                        </domain>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Nom ou nom RC</translation>
                    <description>
                        <short>Nom ou nom RC de l'entreprise.</short>
                        <domain>S'il existe un nom inscrit au registre de commerce, il faut
                            utiliser celui-ci.
                        </domain>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ZIP" type="sdtab:ReducedZIP-CodeType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>PLZ Unternehmen</translation>
                    <description>
                        <short>Postleitzahl</short>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>CP Entreprise</translation>
                    <description>
                        <short>Code postal</short>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    <!--
        PersonIDType -->
    <xs:complexType name="PersonIDType">
        <xs:choice>
            <xs:element name="SV-AS-Nr" type="sdtab:SV-AS-NumberType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Sozialversicherungsnummer (Prio 2)</translation>
                        <description>
                            <short>Von der Zentrale-Ausgleichsstelle (ZAS) zugeteilte Sozialversicherungsnummer</short>
                            <domain>Ab Einführung ist die 13-stellige Sozialversicherungsnummer zu erfassen.
                                Diese wird von der Zentralen Ausgleichsstelle zugeteilt.
                            </domain>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Numéro d'assurance sociale (2eme priorité)</translation>
                        <description>
                            <short>Numéro d'assurance sociale attribué par la centrale de
                                compensation (CdC)
                            </short>
                            <domain>A partir de son introduction, le numéro d'assurance sociale à 13
                                chiffres doit être saisi. Le numéro est attribué par la centrale de
                                compensation (CdC).
                            </domain>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="DateOfBirth" type="xs:date">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>Geburtsdatum (Prio 3)</translation>
                        <description>
                            <short>Geburtsdatum der Person, sofern SV- oder AHV-Nr unbekannt sind</short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>Date de naissance (3eme priorité)</translation>
                        <description>
                            <short>Date de naissance de la personne au cas où les numéros AS et AVS
                                ne sont pas connus.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="unknown" type="sdtab:EmptyType">
                <xs:annotation>
                    <xs:documentation xml:lang="de">
                        <translation>unbekannt (Prio 4)</translation>
                        <description>
                            <short>Die AHV-Nummer und die Sozialversicherungsnummer und das Geburtsdatum sind
                                unbekannt.
                            </short>
                        </description>
                    </xs:documentation>
                    <xs:documentation xml:lang="fr">
                        <translation>inconnu (4eme priorité)</translation>
                        <description>
                            <short>Die AHV-Nummer und die Sozialversicherungsnummer und das
                                Geburtsdatum sind unbekannt.
                            </short>
                        </description>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:choice>
        <xs:attribute name="Lastname" type="sdtab:LastnameType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>Nachname</translation>
                    <description>
                        <short>Vollständiger Familienname der Person</short>
                        <domain>Name gemäss amtlichem Papier (z.B. Versicherungsnachweis)</domain>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Nom</translation>
                    <description>
                        <short>Nom complet de la personne</short>
                        <domain>Nom selon papier officiel (p.e. attestation d'assurance)</domain>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="Firstname" type="sdtab:FirstnameType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>Vorname</translation>
                    <description>
                        <short>Vorname(n) der Person</short>
                        <domain>Vorname gemäss amtlichem Papier (z.B. Versicherungsnachweis)</domain>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>Prénom</translation>
                    <description>
                        <short>Prénom(s) de la personne</short>
                        <domain>Prénom selon papier officiel (p.e. attestation d'assurance)
                        </domain>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
        <xs:attribute name="ZIP" type="sdtab:ReducedZIP-CodeType" use="required">
            <xs:annotation>
                <xs:documentation xml:lang="de">
                    <translation>PLZ Wohnort der Person</translation>
                    <description>
                        <short>Postleitzahl</short>
                    </description>
                </xs:documentation>
                <xs:documentation xml:lang="fr">
                    <translation>CP du lieu de la personne</translation>
                    <description>
                        <short>Code postal</short>
                    </description>
                </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
